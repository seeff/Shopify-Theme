$(window).load(function() {
	if(!$.cookie('newsletter')) {
		$('#dothepopup').trigger('click');
	}
    $("#owl").owlCarousel({ 
        //autoPlay: 3000, //Set AutoPlay to 3 seconds
        navigation: true,
        items : 5,
        itemsDesktop : [1199,3],
        itemsDesktopSmall : [979,3]
   
    });
});

jQuery(function($){
  
  $(".u").unveil(200, function() {
	  $(this).load(function() {
	  	$(this).parent().fitVids();
	    this.style.opacity = 1;
	  });
	});
	

  var allPanels = $('.accordion  ul').hide();
  
  $('#home-video,.rte').fitVids();
    
  $('.accordion li a').click(function() {
    allPanels.slideUp();
    $(this).next().slideDown();
    return false;
  });
	
  

  $('html').removeClass('no-js');
                    
  /* Placeholder JS */
  /*==========================*/

  $('[placeholder]').each(function(){
    if ($(this).val() === '') {
      var hint = $(this).attr('placeholder');
      $(this).val(hint).addClass('hint');
    }
  });

  $('[placeholder]').focus(function() {
    if ($(this).val() === $(this).attr('placeholder')) {
      $(this).val('').removeClass('hint');
    }
  }).blur(function() {
    if ($(this).val() === '') {
      $(this).val($(this).attr('placeholder')).addClass('hint');
    }
  });

  /* Form validation JS */
  /*==========================*/

  $('input.error, textarea.error').focus(function() {
    $(this).removeClass('error');
  });

  $('form :submit').click(function() {
    $(this).parents('form').find('input.hint, textarea.hint').each(function() {
      $(this).val('').removeClass('hint');
    });
    return true;
  });


  //$('#add').bind('click', fastadd	);
  $('.cartremove').bind('click',fastremove);
	
  $('a:not(#triggerzenbox,.various)').each(function() {
    var a = new RegExp('/' + window.location.host + '/');
    if(!a.test(this.href)) {
      $(this).click(function(event) {
        event.preventDefault();
        event.stopPropagation();
        window.open(this.href, '_blank');
      });
    }
  });
	
  
  	$('#accordion > ul > li:has(ul)').addClass('has-sub');
	$('.has-sub > a').append('<b>+</b>');
    $('#accordion > ul > li > a.active').next().show();
  	$('#accordion > ul > li > a.active').find('b').text('-');
  	$('#accordion > ul > li > ul > li > a.active').parent().parent().show();
  
  
  
  	$('#country-list a').click(function() {
    	$.cookie('language', $(this).data('language'), { expires: 7, path: '/' });
    	return true;
  	});
                                      
$('a.fancybox').fancybox({
  autoSize	    : true,
  closeClick	: false,
  openEffect	: 'none',
  closeEffect	: 'none',
    afterClose: function() {
    	$.cookie('newsletter', 'tryagain', { expires: 2, path: '/' });
    }
});
  
  $("#p-signup").submit(function(e) {
    $.cookie('newsletter','signedup', {expires: 365, path: '/' });
    return true;
  });
	 
  
  $('#mobile-nav,#mobile-cart').hide();
  $('#menu-btn').click(function(e) {
    e.preventDefault();
    $('#mobile-nav').slideToggle();
    if($('#mobile-cart:visible')) $('#mobile-cart').hide();
  });
  $('#mobile-cart-btn').click(function(e) {
    e.preventDefault();
    $('#mobile-cart').slideToggle();
    if($('#mobile-nav:visible')) $('#mobile-nav').hide();
  });
  $('.search-btn').click(function(e) {
    e.preventDefault();
    $('#search-form').slideToggle();
  });

});

function fastadd(e){
	e.preventDefault();
  	var form = $(this).parents('form');
	var $addedtext = "Added to Cart";
	var $addtext = $(this).val();
	var $addcolor = $(this).css('color');
	var $returntime = 2000;
	
	var $add = $('#add');
	$add.val($addedtext);
	
	var AJXPage = '/search';
  	$.ajax({
    	type: 'POST',
      	url: '/cart/add.js',
		async: false,
      	data: form.serialize(),
      	dataType: 'json',
      	success: function(response){
        	$.ajax({
              	type: 'GET',
              	url: '/cart.js',
              	dataType: 'json',
              	success: function(cartdata){
					$.get(AJXPage, function(data) {
						$newcart = $(data).find('#top-cart');
						$curcart = $('#top-cart');
						$curcart.html($newcart.html()).promise().done(function() {
							$('.cartremove').bind('click',fastremove );
							setTimeout(function() {
								$add.val($addtext).removeAttr('disabled').css('color', $addcolor);
							}, $returntime);
						});
						
					});
              	}
          	});
      	},
	  	error: function(response) {
		
		}
  	});
}

function fastremove(e) {
	e.preventDefault();
	var AJXPage = '/search';
	removeURL = $(this).attr('href');
    $.post(removeURL,function(data) {
        $.get(AJXPage, function(data){
            var $newcart = $(data).find('#top-cart');
            var $curcart = $('#top-cart').html($newcart.html());
        });
    });
}

